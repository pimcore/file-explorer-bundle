{
  "name": "pimcore/file-explorer-bundle",
  "license": "GPL-3.0+",
  "type": "pimcore-bundle",
  "description": "Pimcore File Explorer Bundle allows to read/write files on the server from within the Pimcore admin interface.",
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "pimcore/pimcore": "^11.0",
    "pimcore/admin-ui-classic-bundle": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Pimcore\\Bundle\\FileExplorerBundle\\": "src/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Pimcore\\Bundle\\FileExplorerBundle\\PimcoreFileExplorerBundle"
      ]
    }
  }
}
